#format: sh
# 
# Configuration file (mgiconfig) for mgigff product.
# See also: config.cfg in this directory.
#

if [ "${MGICONFIG}" = "" ]
then
    MGICONFIG=/usr/local/mgi/live/mgiconfig
    export MGICONFIG
fi

. ${MGICONFIG}/master.config.sh

## Set the name of the blat server to use
#
BLAT_HOST=${BLAT_HOST_DEV}
if [ "${INSTALL_TYPE}" = prod ]
then
   BLAT_HOST=${BLAT_HOST_PROD}
fi
